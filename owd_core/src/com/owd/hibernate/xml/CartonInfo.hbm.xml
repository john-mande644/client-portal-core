<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

    <class name="com.owd.hibernate.generated.CartonInfo" table="carton_info">
        <id
                name="num"
                type="java.lang.Integer"
                column="num"
        >
            <generator class="identity"/>
        </id>

        <property name="SSCC" column="sscc"
                  type="java.lang.String"
                  length="255" not-null="true">
        </property>

        <property name="CartonNum" column="carton_num"
                  type="java.lang.Integer"  not-null="true">
        </property>
    </class>
</hibernate-mapping>