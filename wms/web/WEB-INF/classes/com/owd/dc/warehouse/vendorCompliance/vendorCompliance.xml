<struts>


    <package name="vendorCompliance" namespace="/vendorCompliance" extends="struts-default">
        <result-types>
            <result-type name="customJSON" class="com.owd.results.JSONResult"/>
        </result-types>
        <action name="labelMaker" method="labelMaker" class="com.owd.dc.warehouse.vendorCompliance.labelsMaker.labelMakerAction">

            <result name="success">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
            <result name="error">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
        </action>
        <action name="loadVendors" method="loadVendors" class="com.owd.dc.warehouse.vendorCompliance.labelsMaker.labelMakerAction">

            <result name="success">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
            <result name="error">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
        </action>
        <action name="loadClients" method="loadClients" class="com.owd.dc.warehouse.vendorCompliance.labelsMaker.labelMakerAction">

            <result name="success">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
            <result name="error">/warehouse/vendorCompliance/labelMaker/index.jsp</result>
        </action>


    </package>

</struts>