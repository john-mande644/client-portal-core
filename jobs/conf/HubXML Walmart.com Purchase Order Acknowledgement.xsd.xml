    <messageControlNumber>string</messageControlNumber>
    <originatingSystemTrxId trxDate="string" trxType="string" systemOwner="string" systemName="string">string</originatingSystemTrxId>
    <!--1 or more repetitions:-->
    <participatingParty name="string" role="merchant" participationCode="To:">string</participatingParty>
    <poNumber>string</poNumber>
    <scheduledShipDate>string</scheduledShipDate>
    <!--1 or more repetitions:-->
